    "@modelcontextprotocol/server-filesystem": "^2025.3.28",
    "@mozilla/readability": "^0.6.0",
    "blessed": "^0.1.81",
    "dayjs": "^1.11.13",
    "elytra-ts": "^0.1.6",
    "eventsource": "^4.0.0",
    "express": "^5.1.0",
    "googleapis": "^105.0.0",
    "jsdom": "^26.1.0",
    "kysely": "^0.28.2",
    "langchain": "^0.3.29",
